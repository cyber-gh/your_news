<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_headlines">
    <fragment
        android:id="@+id/navigation_headlines"
        android:name="dev.skyit.yournews.ui.main.newsheadlines.NewsHeadlinesFragment"
        android:label="NewsHeadlinesFragment"
        tools:layout="@layout/news_headlines_fragment">
        <action
            android:id="@+id/action_navigation_headlines_to_searchNewsFragment"
            app:destination="@id/searchNewsFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_bookmarks"
        android:name="dev.skyit.yournews.ui.main.bookmarks.NewsBookmarksFragment"
        android:label="NewsBookmarksFragment"
        tools:layout="@layout/news_bookmarks_fragment"/>
    <fragment
        android:id="@+id/navigation_sources"
        android:name="dev.skyit.yournews.ui.main.sources.NewsSourcesFragment"
        android:label="NewsSourcesFragment"
        tools:layout="@layout/news_sources_fragment"/>
    <fragment
        android:id="@+id/searchNewsFragment"
        android:name="dev.skyit.yournews.ui.main.search.SearchNewsFragment"
        android:label="SearchNewsFragment"
        tools:layout="@layout/search_news_fragment"/>
</navigation>